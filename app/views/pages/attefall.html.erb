<% @size = 12 %>

<div class="container">
  <div class="row justify-content-center mb-5">
    <div class="card-columns">
      <% @images.each do |image| %>
      <div class="card">
        <div class="card-body p-0">
          <a href="<%= '/images/attefall/' + image %>" target="_blank">
            <% image = image.split('.')[0] + '_sm.' + image.split('.')[1] %>
            <img src="<%= '/images/attefall/' + image %>">
          </a>
        </div>
      </div>
      <% end %>
    </div>
  </div>
</div>
